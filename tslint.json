{
  "extends": ["tslint-config-alloy", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false, // 导入模块不用按字典序
    "member-access": false, // 允许不写 public protected private
    "no-object-literal-type-assertion": false, // 允许对对象字面量使用类型断言
    "comment-format": false, // 评论 不要格式
    "no-any": false,
    "no-angle-bracket-type-assertion":false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
